body {
  background-image: url("data:image/jpeg;base64,");
  margin: 0;
  padding: 0;
}
.row2 {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  border-radius: 10px;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
  padding: 10px 0;
  margin: 20rpx;
}
.row2 .item {
  display: flex;
  align-items: center;
  padding: 10px 20px;
  border-bottom: 1px solid #f0f0f0;
}
.row2 .item:last-child {
  border-bottom: none;
}
.row2 .item .left {
  display: flex;
  align-items: center;
  margin-right: 20px;
  font-weight: bold;
}
.row2 .item .left text {
  font-size: 16px;
  color: #333;
}
.row2 .item .right {
  flex-grow: 1;
  text-align: right;
  color: #666;
  font-size: 14px;
}
.row2 .item .right image {
  width: 40px;
  height: 40px;
}